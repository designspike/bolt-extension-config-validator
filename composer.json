{
    "name": "designspike/bolt-extension-config-validator",
    "description": "Adds a menu item to validate your Bolt config files.",
    "type": "bolt-extension",
    "keywords": ["configuration", "utility", "debugging"],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Will Hall",
            "email": "will@designspike.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\DesignSpike\\ConfigValidator\\": "src",
            "RomaricDrigon\\MetaYaml\\": "lib/RomaricDrigon/MetaYaml/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\DesignSpike\\ConfigValidator\\ConfigValidatorExtension"
    }
}
